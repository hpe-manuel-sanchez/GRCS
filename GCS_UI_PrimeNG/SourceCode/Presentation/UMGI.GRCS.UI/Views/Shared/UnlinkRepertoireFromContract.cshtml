@using UMGI.GRCS.Resx.Resource.UIResources
<link href="@Url.Content("/GCS/Scripts/jtable/themes/standard/blue/jtable_blue.min.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("/GCS/Content/SubPages/UnlinkRepertoireFromContract.min.css")"  rel="stylesheet" type="text/css" />
<script src="@Url.Content("/GCS/Scripts/jtable/jquery.jtable.js")" type="text/javascript"></script>
<script src="@Url.Content("/GCS/Scripts/Custom/UnlinkRepertoireFromContract.js")" type="text/javascript"></script>

<div id="unlinkWarning">
    <div class="warningContainer">
        <img class="alertImage" src="/GCS/Images/Alert.png" alt="Warning" />
    </div>
    <div id="UnlinkAlert"></div>
</div>
<div class="scrollUnlinkRep">
    <div id="containerTop">
        <div>
            <p>@ContractResource.SelectProjectRepertoireHeader</p>
            <p>
                <input type="checkbox" id="selectAllRepertoire" />
                @ContractResource.AllRepertoireCheckBox<span class="alignRight"><input type="submit" class="primbutton btnUnlinkContract" value="@ContractResource.ConfirmUnlinkButton" />
                    <input type="submit" class="btncancelUnlink secbutton btnSpacing" value="Cancel" /></span>
            </p>
        </div>
    </div>
    <div id="accordion">
        <div id="project">
            <div class="head">
                <div class="leftRight">
                    <a href="#" class="header alignLeft"><span id="unlinkProjCount">@ContractResource.ProjectHeader </span></a>
                </div>
            </div>
            <div>
                <div id="unlinkProjectGrid">
                </div>
            </div>
        </div>
        <div id="release">
            <div class="head">
                <div class="leftRight">
                    <a href="#" class="header alignLeft"><span id="unlinkRelCount">@ContractResource.ReleaseHeader </span></a>
                </div>
            </div>
            <div>
                <div id="unlinkReleaseGrid">
                </div>
            </div>
        </div>
        <div id="resource">
            <div class="head">
                <div class="leftRight">
                    <a href="#" class="header alignLeft"><span id="unlinkResCount">@ContractResource.ResourceHeader </span></a>
                </div>
            </div>
            <div>
                <div id="unlinkResourceGrid">
                </div>
            </div>
        </div>
        
    </div>
</div>
<div class="unlinkFooter">
    <div id="unlinkFooterBtnContainer">
        <input type="submit" class="primbutton btnUnlinkContract" value="@ContractResource.ConfirmUnlinkButton" />
        <input type="submit" class="btncancelUnlink secbutton btnSpacing" value="@ContractResource.CancelButton" />
    </div>
</div>