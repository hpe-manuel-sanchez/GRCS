@model UMGI.GRCS.UI.Models.WorkgroupModel
@{
    Layout = null;
}
<!DOCTYPE html>
<link href="@Url.Content("/GCS/Scripts/jtable/themes/standard/blue/jtable_blue.min.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("/GCS/Scripts/Custom/CreateParentWorkgroup.js")" type="text/javascript"></script>
<script src="@Url.Content("/GCS/Scripts/jquery.watermark.js")" type="text/javascript"></script>

<div id="manageUsers" style="margin-left: 15px;">
    <div style="width: 80%;">
        <div class="accItem">
            <div class="editor-label">
            </div>
            <div class="editor-field">
                @Html.Label("Search by:")
                @Html.TextBox("username", "", new { maxlength = "100" })
                @Html.Editor("userid", "", new { maxlength = "8" })
                @Html.Editor("usercountry", "", new { maxlength = "40" })
                <input id="btnOpenSearchUser" type="button" value="Search" runat="server" />
                <input id="btnManageUserReset" type="button" value="Reset" runat="server" />
            </div>
        </div>
    </div>
    <div id="addedManageUserResults">
    </div>
    <div style="float: left;">
        <input id="btnRemove" type="button" value="Remove" runat="server" />
    </div>
    <div style="float: right;">
        <input id="btnUserSave" type="button" value="Save" runat="server" />
        <input id="btnUserCancel" type="button" value="Cancel" runat="server" />
    </div>
</div>