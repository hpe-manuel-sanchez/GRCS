    function applyCustomTheam() {

        var docHeight = $(window).height();
        var headerHeight = 60;
        var footerHeight = 35;

        var mainContainerHeight = docHeight - headerHeight - footerHeight;

        $("#main").css({ "height": mainContainerHeight + "px", "overflowY": "auto", "overflowX": "hidden" });

    }
    applyCustomTheam();
