@model UMGI.GRCS.UI.Models.CustomWorkQueueModel
@using UMGI.GRCS.Resx.Resource.UIResources
<script src="@Url.Content("/GCS/Scripts/Custom/CustomSetting.js")" type="text/javascript"></script>
<script src="@Url.Content("/GCS/Scripts/json2.js")" type="text/javascript"></script>
<link href="@Url.Content("/GCS/Content/SubPages/CustomSetting.min.css")"  rel="stylesheet" type="text/css" />
<style>
    .ui-tabs .ui-tabs-nav li
    {
        list-style: none;
        float: left;
        position: relative;
        top: 0px;
        top: 2px\9;
        border: none; /*margin: 0 .65em 1px 0;*/ /*border-bottom: 0 !important; padding: 0; white-space: nowrap;
                            
                            border-top: 1px solid #B9B9B9;
                            border-left: 1px solid #B9B9B9;
                            border-right: 1px solid #B9B9B9;*/
    }
</style>
<div id="splitWarning">
    <div class="warningContainer">
        <span class="alertImage"></span></div>
    <div id="SplitAlert">
    </div>
</div>
<div id="customSettingTab">
    <div id="menuTab">
        <div class="alignLefte">
            <ul class="tabul" id="popUpTabs">
                <li><a href="#divOtherParameterTab">@WorkQueueResource.CustomParameters </a></li>
                <li><a href="#divPredefinedParameter">@WorkQueueResource.Predefined </a></li>
            </ul>
        </div>
    </div>
    <div id="divPredefinedParameter" class="" style="clear: both;">
       
        @Html.Partial(@WorkQueueResource.PredefindParameterTab, Model)
    </div>
    <div id="divOtherParameterTab" class="" style="clear: both;">
        @Html.Partial(@WorkQueueResource.OtherParameterTab, Model)
    </div>
</div>
<div id="CustompopUpFooter" class="clearBoth">
    <div id ="bottomResetSelection"style="float:left;"> 
         <span class="hyperlink"><div class="resetImg" style="display: block"></div><a id="resetSelection" href="javascript:void(0);">@WorkQueueResource.ResetSelection</a></span> 
    </div>
    <div id="bottomButtonss">
        <div style="margin-right:10px; float:left;">        <div class="primbutton_left">
                                                            </div>
            <div class="alignLeft">
                <input type="submit" class="primbutton" value='@WorkQueueResource.CreateWorkQueueValue' id="crtWorkQueue" />
            </div>
            <div class="primbutton_right"> </div>
        </div>

        <div style="float:left;">
            <div class="secbutton_left"></div>
            <div class="alignLeft">
                <input type="submit" class="secbutton" value="Cancel" id="cancelWorkQueue" /></div>
            <div class="secbutton_right"></div>
        </div>
    </div>
</div>
