                    <td align="left"><select size="1" style="width:200px"><option>---select---</option></select></td>
                                          
                </tr> 
                  <tr>    
                                     
                    <td align="right" colspan="2"><b>ICLA Level:</b></td>    
                    <td align="left">@Html.Label("Top")</td>         
                    <td align="center" colspan="3">@Html.TextArea("txtRegAreaComments", "Comments")</td>     
                                        
                </tr> 
                 <tr>       
                                 
                      <td align="left" colspan="6"><b>TV/Radio Break ICLA</b></td>   
                      
                </tr>
