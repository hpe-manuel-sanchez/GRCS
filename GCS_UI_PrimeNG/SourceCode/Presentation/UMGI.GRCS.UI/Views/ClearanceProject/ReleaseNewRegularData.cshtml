                    <td align="left">@Html.TextBox("txtDeemedPPD")</td>                          
                </tr>     
                 <tr>             
                        
                    <td align="right"><b>Invoice Price:</b></td>                        
                    <td align="left">@Html.TextBox("txtInvPrice")</td>
                    <td align="right"><b>ICLA Accounting Base:</b></td> 
                    <td align="left" colspan="3">@Html.TextBox("txtICLAcc")</td>                           
                                          
                </tr>     
                 <tr>  
                                  
                    <td align="right"><b>Selling Price Less VAT:</b></td>    
                   <td align="left" colspan="5">@Html.TextBox("txtSellPrice")</td>    
                    
                                        
