@*@model UMGI.GRCS.UI.Models.ClearanceProjectModel
           
 <table style="width: 100%;">        
         

        <tr>           

            <td>
                <table style="width: 100%; border:no;">
                 <tr>
                 
                            <td align="right"><b>Label:</b></td>
                            <td align="left">@Html.DropDownListFor(model => model.LabelList, new SelectList(Model.LabelList, "value", "text", Model.LabelList), "--Select--")</td> 

                            <td align="right"><b>Music Type:</b></td>
                            <td align="left">@Html.DropDownListFor(model => model.MusicType, new SelectList(Model.MusicType, "value", "text", Model.MusicType), "--Select--")</td>
                                                        
                            <td align="right"><b>Package:</b></td>                           
                            <td align="left" width="20px">Package</td>    
                    </tr>
                    <tr>
                            
                            <td align="right"><b>Configuration Group:</b></td>
                            <td align="left">@Html.DropDownListFor(model => model.ConfigGroupList, new SelectList(Model.ConfigGroupList, "value", "text", Model.ConfigGroupList), "--Select--")</td>
                                  
                            <td align="right"><b>Sound Track:</b></td>                               
                            <td align="left">@Html.CheckBox("chkSoundTrack")</td> 

                            <td align="right"><b>Number Of Components:</b></td>
                            <td align="left">@Html.TextBox("txtNoOfComp")</td>  
                                                                                            
                    </tr>
                    <tr>
                            
                            <td align="right"><b>Configuration:</b></td>
                            <td align="left">@Html.DropDownListFor(model => model.ConfigList, new SelectList(Model.ConfigList, "value", "text", Model.ConfigList), "--Select--")</td>
                                                         
                            <td align="right"><b>Number Of Tracks:</b></td>
                            <td align="left">@Html.TextBox("txtNoOfTrack")</td> 
                              
                            <td align="right"><b>Existing Releases in<br />Package:</b></td>  
                            <td align="left">
                            <select id="cmbExRelInPkg" runat="server">
                            <option>-select-</option>
                            <option id="1" value="Yes">Yes</option>
                            <option id="2" value="No">No</option>
                            </select>
                            <input type="button" value="Search" />
                            </td>
                </tr>

                <tr>
                        
                        <td colspan="5">&nbsp;</td>
                        <td>@Html.ActionLink("(View Existing Release)", "", "")</td>
                </tr>

                
                <tr>        
                        <td colspan="5">&nbsp;</td>
                        <td>@Html.TextArea("txtAreaComments", "") </td>
                </tr>

