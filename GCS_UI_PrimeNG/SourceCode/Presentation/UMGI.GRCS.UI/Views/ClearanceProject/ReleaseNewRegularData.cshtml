                </tr> 
                <tr>      
                                
                    <td align="right" colspan="2"><b>Exact PPD:</b></td>    
                    <td align="left" colspan="4">@Html.TextBox("txtExPPD")</td>     
                </tr> 
                 <tr>       
                                  
                    <td align="right" colspan="2"><b>Estimated Retail:</b></td>    
                    <td align="left" colspan="4">@Html.TextBox("txtEstRetail")</td>                    
                </tr> 
                <tr>         
                              
                      <td align="left" colspan="6"><b>Price Reduction</b></td>   
                      
                </tr>
