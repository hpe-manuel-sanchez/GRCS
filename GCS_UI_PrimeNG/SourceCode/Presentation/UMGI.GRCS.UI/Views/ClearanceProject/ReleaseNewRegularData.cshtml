                  <tr>      
                                
                    <td align="right"><b>ICLA Level:</b></td>    
                    <td align="left">@Html.Label("Top")</td>         
                    <td align="right" colspan="2">@Html.TextArea("txtNonTradAreaComments", "Comments")</td>     
