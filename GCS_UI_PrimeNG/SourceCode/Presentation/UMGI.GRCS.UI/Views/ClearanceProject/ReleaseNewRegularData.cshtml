                    <td align="left" colspan="2"><select size="1" style="width:200px"><option>---select---</option></select></td>
                                        
                </tr> 
                  <tr>   
                                 
                    <td align="right" colspan="2"><b>ICLA Level:</b></td>    
                    <td align="left">@Html.Label("Top")</td>         
                    <td align="center" colspan="3">@Html.TextArea("txtPromoAreaComments", "Comments")</td>     
                                         
                </tr>  
                <tr></tr>           
                 <tr>      
                           
                      <td align="left" colspan="6"><b>Non Traditional</b></td>   
                      
                </tr>
                 <tr>                 
                     
                    <td align="left"><b>@Html.RadioButton("rdoRadio","") ICLA </b></td>
